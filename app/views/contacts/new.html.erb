<h1>New Contact</h1>

<%= form_tag "/contacts", method: :post do %>

<div> 
  First Name: <input type="text" name="first name">
</div>

<div> 
  Middle Name: <input type="text" name="middle_name">
</div>

<div> 
  Last Name: <input type="text" name="last name">
</div>

<div> 
  Email: <input type="text" name="email">
</div>

<div> 
  Phone: <input type="text" name="phone_number">
</div>

<div> 
  Bio: <input type="text" name="bio">
</div>

<input type="submit" value="sumbmit changes">

<% end %>